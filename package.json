{
  "name": "lingerie-website",
  "version": "1.0.0",
  "type": "module",
  "description": "Modern lingerie e-commerce website built with React and Vite",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
